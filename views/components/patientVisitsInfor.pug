div#pageNumber Visits: 
    - for(var x = 1; x <= page; x++)
        button.pageNum(onclick='changePage(' + x + ')') #{x}
div.content-header
    h1 Visit History
div.content-grid.visit-grid
    div.headlines
        h2 Checkin
        h2 Checkout
        h2 
        h2 Contents
    div.visitInfo
    each visit in visits
        div.visit
            span= visit.checkin
            span= visit.checkout
            button(onclick='viewVisitDetail(' + visit.id +')') Specific Information
            div.visit-specific
                div(class="visit-detail" id="visit-detail" + visit.id)
