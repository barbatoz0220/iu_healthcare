div#visitInfor
    div#visit-headlines
        h2 Checkin
        h2 Checkout
        h2 
        h2 Contents
    div.visitInfo
    each visit in visits
        div.patient-visit
            span= visit.checkin
            span= visit.checkout
            button(onclick='viewVisitDetail(' + visit.id +')') Specific Information
            div.visit-specific
                div(class="visit-detail" id="visit-detail" + visit.id)

